-                             &delayed_usecs);
+    jack_nframes_t nframes; 
+
+    wait_status = 0;
+    delayed_usecs = 0;
+
+    nframes = net_driver_wait (driver, -1, &wait_status, &delayed_usecs);
 
     // currently there is no xrun detection.
