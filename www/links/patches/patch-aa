 /* this doesn't send referer */
 void goto_url(struct session *ses, unsigned char *url)
 {
+	char *p;
+	p = url + strlen(url);
+	do {
+		--p;
+	} while (isspace(*p));
+	*++p = 0;
+
 	goto_url_f(ses, NULL, url, NULL, NULL, -1, 0, 1, 0);
 }
 
