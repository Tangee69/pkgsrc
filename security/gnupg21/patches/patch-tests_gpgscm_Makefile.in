@@ -458,7 +458,7 @@ gpgscm_SOURCES = main.c private.h ffi.c 
 
 gpgscm_LDADD = $(LDADD) $(common_libs) \
 	$(NETLIBS) $(LIBICONV) $(LIBREADLINE) \
-	$(LIBGCRYPT_LIBS) $(GPG_ERROR_LIBS)
+	$(LIBGCRYPT_LIBS) $(GPG_ERROR_LIBS) $(LIBINTL)
 
